(deftemplate component
 	(slot id)
	(slot name)
 	(slot state)
	(slot type)
	(multislot connected-to)
)
(deftemplate connection
 	extends component
	(slot in)
	(slot out)
)
(deftemplate feature
 	(slot id)
 	(slot name)
)
(deftemplate observable
	extends feature
	(slot could-be-observed)
	(slot observed)
)
(deftemplate state
	extends feature
	(slot faulty)
)
(deftemplate causes-to-wire
	(slot faulty-comp-type)
	(slot faulty-comp-state)
	(slot wire-state)
)
