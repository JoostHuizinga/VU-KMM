(deftemplate causes-from-wire
	"Causal relationship from a wire to a component."
 	(slot faulty-wire-state)
	(slot component-type)
	(slot component-state)
)
(deftemplate causes-from-2wires
	"Causal relationship from 2 wire to a component."
 	(slot faulty-wire1-state)
 	(slot faulty-wire2-state)
	(slot component-type)
	(slot component-state)
)
(deftemplate causes-feature
	"Causal relationship from a component to a feature."
	(slot component)
	(slot component-state)
	(slot observable)
)
(deftemplate causes-to-wire
	"Causal relationship from a component to a wire."
	(slot faulty-comp-type)
	(slot faulty-comp-state)
	(slot wire-state)
)
