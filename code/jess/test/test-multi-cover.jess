(defquery search-components
	(component 
		(id ?component)
		(state ?state)
		(possible-states $?possible-states)
		(impossible-states $?impossible-states)
	)
)

(defquery components-in-state
	(component 
		(id ?component)
		{state <> nill}
	)
)