(defrule obtained-complaint1
	""
	(complaint ?oid TRUE)
	?o <- (observable
		{id == ?oid}
	)
	=>
	(modify ?o
		(observed TRUE)
		(complaint TRUE)
	)
)
(defrule obtained-observation2
	""
	(observed ?oid FALSE)
	?o <- (observable
		{id == ?oid}
	)
	=>
	(modify ?o
		(observed FALSE)
		(complaint TRUE)
	)
)
